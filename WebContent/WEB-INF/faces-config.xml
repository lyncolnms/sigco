<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
 <converter>
  <converter-id>entityConverter</converter-id>
  <converter-class>br.embrapa.cnpso.sigco.model.utils.EntityConverter</converter-class>
 </converter>
 <converter>
  <converter-id>autorizacaoConverter</converter-id>
  <converter-class>br.embrapa.cnpso.sigco.model.utils.AutorizacaoConverter</converter-class>
 </converter>
 <converter>
  <converter-id>areaConverter</converter-id>
  <converter-class>br.embrapa.cnpso.sigco.model.utils.AreaConverter</converter-class>
 </converter>
 <converter>
  <converter-id>estadosConverter</converter-id>
  <converter-class>br.embrapa.cnpso.sigco.model.utils.EstadosConverter</converter-class>
 </converter>
 <managed-bean>
  <managed-bean-name>subareaBean</managed-bean-name>
  <managed-bean-class>br.embrapa.cnpso.sigco.controller.SubareaAtuacaoBean</managed-bean-class>
  <managed-bean-scope>view</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>areaBean</managed-bean-name>
  <managed-bean-class>br.embrapa.cnpso.sigco.controller.AreaAtuacaoBean</managed-bean-class>
  <managed-bean-scope>view</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>autorizacaoBean</managed-bean-name>
  <managed-bean-class>br.embrapa.cnpso.sigco.controller.AutorizacaoBean</managed-bean-class>
  <managed-bean-scope>view</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>usuarioBean</managed-bean-name>
  <managed-bean-class>br.embrapa.cnpso.sigco.controller.UsuarioBean</managed-bean-class>
  <managed-bean-scope>view</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>equipeTecnicaBean</managed-bean-name>
  <managed-bean-class>br.embrapa.cnpso.sigco.controller.EquipeTecnicaBean</managed-bean-class>
  <managed-bean-scope>view</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>funcaoBean</managed-bean-name>
  <managed-bean-class>br.embrapa.cnpso.sigco.controller.FuncaoBean</managed-bean-class>
  <managed-bean-scope>view</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>localBean</managed-bean-name>
  <managed-bean-class>br.embrapa.cnpso.sigco.controller.LocalBean</managed-bean-class>
  <managed-bean-scope>view</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>loginLogoutBean</managed-bean-name>
  <managed-bean-class>br.embrapa.cnpso.sigco.controller.LoginLogoutBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <application>
  <el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
 </application>
</faces-config>
